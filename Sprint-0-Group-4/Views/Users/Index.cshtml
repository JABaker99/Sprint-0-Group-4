@model List<Sprint_0_Group_4.Models.AppUser>

@{
    ViewData["Title"] = "Users";
}

<h2>Users from Database</h2>

@if (Model.Any())
{
    <table border="1" cellpadding="5">
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Firstname</td>
                <td>@user.Lastname</td>
            </tr>
        }
    </table>
}
else
{
    <p>No users found</p>
}

